  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/ini": "1.3.30",
    "@types/jest": "25.2.1",
    "@types/mock-fs": "4.10.0",
    "@types/node": "10.17.5",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "auto-changelog": "2.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.5.4",
    "mock-fs": "4.12.0",
    "pkg": "4.4.8",
